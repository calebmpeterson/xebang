<?xml version="1.0" encoding="UTF-8"?>
<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/">
    <ShortName>Xebang With Suggestions</ShortName>
    <LongName>Xebang Web Search With Google Search Suggestions</LongName>
    <Description>Xebang provides custom bangs, passes unrecognized !bang's to DuckDuckGo and everything else to DuckDuckGo</Description>
    <InputEncoding>UTF-8</InputEncoding>
    <OutputEncoding>UTF-8</OutputEncoding>
    <AdultContent>false</AdultContent>
    <Language>en-us</Language>
    <Developer>Caleb Peterson</Developer>
    <Contact>caleb.peterson@cubicle6.com</Contact>
    <Tags>Custom Bangs</Tags>
    <Attribution>Custom Bangs a-top DDG</Attribution>
    <SyndicationRight>open</SyndicationRight>
    <Query role="example" searchTerms="Apple !i"/>
    <Image height="16" width="16" type="image/vnd.microsoft.icon">http://xebang.cubicle6.com/favicon.ico</Image>
    <Url type="text/html" template="http://www.Xebang.com/?q={searchTerms}"/>
    <Url type="application/x-suggestions+json" template="http://google.com/complete/search?output=firefox&amp;q={searchTerms}"/>
    <Url rel="suggestions" type="application/x-suggestions+xml" template="http://google.com/complete/search?q={searchTerms}&amp;client=ie8&amp;mw={ie:maxWidth}&amp;sh={ie:sectionHeight}&amp;rh={ie:rowHeight}&amp;inputencoding={inputEncoding}&amp;outputencoding={outputEncoding}"/>
    <Url type="application/opensearchdescription+xml" rel="self" template="http://Xebang.com/opensearchsuggest.xml" />
</OpenSearchDescription>
